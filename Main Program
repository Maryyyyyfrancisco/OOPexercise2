public class MainProgram{
	public static void main(String[] args){
		Pet pet;
		Cat cat= new Cat("Luffy", 10, "Maine Coon");
		Dog dog=new Dog("Zoro", 10, "Corgi");
		
		pet=Cat;
		System.out.println(pet.makeSound()+" "+"My name is "+pet.getName());
		
		pet=Dog;
		System.out.println(pet.makeSound()+" "+"My name is "+pet.getName());
	}
}
